{
  "id": "react-layer-notes",
  "name": "Layer Notes",
  "version": "1.0.0",
  "main": "index.html",
  "manifestVersion": 4,
  "host": [
    {
      "app": "PS",
      "minVersion": "22.0.0"
    }
  ],
  "entrypoints": [
    {
      "type": "panel",
      "id": "demos",
      "label": {
        "default": "Layer Notes"
      },
      "minimumSize": {
        "width": 230,
        "height": 200
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 230,
        "height": 300
      },
      "preferredFloatingSize": {
        "width": 230,
        "height": 300
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/light.png",
          "scale": [1, 2],
          "theme": ["darkest", "dark", "medium"]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark.png",
          "scale": [1, 2],
          "theme": ["lightest", "light"]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "icons/lightPlugin.png",
      "scale": [1, 2],
      "theme": ["darkest", "dark", "medium"],
      "species": ["pluginList"]
    },
    {
      "width": 48,
      "height": 48,
      "path": "icons/darkPlugin.png",
      "scale": [1, 2],
      "theme": ["lightest", "light"],
      "species": ["pluginList"]
    }
  ]
}